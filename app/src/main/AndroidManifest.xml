<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lading.applicationdemo">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".operator.NormalRxActivity"></activity>
        <activity android:name=".operator.RxMapActivity"></activity>
        <activity android:name=".operator.RxSchuderActivity"></activity>
        <activity android:name=".operator.RxFlatMapActivity"></activity>
        <activity android:name=".operator.RxMergeActivity"></activity>
        <activity android:name=".operator.RxBindingActivity"></activity>
        <activity android:name=".operator.RxFilterActivity"></activity>
        <activity android:name=".operator.RxTakeActivity"></activity>
        <activity android:name=".operator.RxTimerActivity"></activity>
        <activity android:name=".operator.RxSortActivity"></activity>
        <activity android:name=".operator.RxConnetActivity"></activity>
        <activity android:name=".operator.TimestampActivity"></activity>
        <activity android:name=".NetActivity"></activity>

    </application>

</manifest>